{
  "name": "web-tests",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "pnpm test:e2e",
    "start:web": "(cd ../../apps/web && pnpm start)",
    "start:api": "(cd ../../apps/api && pnpm start)",
    "dev:test": "pnpm test:e2e",
    "e2e:test": "start-server-and-test 'pnpm start:web & pnpm start:api' '3100|2022' 'pnpm test:e2e'",
    "test:e2e": "cucumber-js --publish-quiet --require cucumber.conf.js --require steps/**/*.js"
  },
  "dependencies": {
    "playwright": "1.29.2",
    "start-server-and-test": "^1.15.2",
    "web": "0.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.10.0",
    "@cucumber/messages": "*",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@playwright/test": "1.29.2",
    "tsconfig": "0.0.0"
  }
}
